{
  "name": "open-circuit",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "bootstrap": "^4.2.1",
    "cordova-android": "^7.1.4",
    "cordova-browser": "^5.0.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "jquery": "^3.4.1",
    "konva": "^2.6.0",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.6",
    "react": "^16.7.0",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "cordova:browser": "cordova build browser",
    "cordova:android": "cordova build android",
    "build": "npm run build:react && npm run move && npm run add-cordova",
    "build:browser": "npm run build && npm run cordova:browser",
    "build:android": "npm run build && npm run cordova:android",
    "move": "rm -rf www/ && mv build www",
    "add-cordova": "node addCordova.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx tsc --noEmit && npx tslint --project ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/lodash": "^4.14.121",
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.4",
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.2",
    "tslint": "^5.18.0",
    "typescript": "3.2.2"
  }
}